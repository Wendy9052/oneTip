AHX,SADK,CARF;AAeL,QAAA,UAAU,EAAE,CACV;AACE,UAAA,QAAQ,EAAE,IADZ;AAEE,UAAA,OAAO,EAAE,yBAFX;AAGE,UAAA,OAAO,EAAE;AAHX,SADU,CAfP;AAsBL,QAAA,QAAQ,EAAE,CACR;AACE,UAAA,QAAQ,EAAE,IADZ;AAEE,UAAA,OAAO,EAAE,4BAFX;AAGE,UAAA,OAAO,EAAE;AAHX,SADQ;AAtBL;AAfF,KAAP;AA8CD,GAnDY;AAoDb,EAAA,OApDa,qBAoDH,CAAE,CApDC;AAqDb,EAAA,OAAO,EAAE;AACP;AACA,IAAA,YAFO,0BAEQ;AACb,WAAK,cAAL,GAAsB,KAAtB;AACA,WAAK,SAAL,GAAiB,KAAjB;AACD,KALM;AAMP;AACA,IAAA,UAPO,wBAOM;AAAA;;AACX,WAAK,cAAL,GAAsB,KAAtB;AACA,MAAA,KAAK,CAAC;AACJ,QAAA,GAAG,EAAE,cADD;AAEJ,QAAA,OAAO,EAAE;AACP,0BAAgB;AADT;AAFL,OAAD,CAAL,CAKG,IALH,CAKQ,UAAC,GAAD,EAAS;AACf,QAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ,EAAkB,GAAG,CAAC,IAAJ,CAAS,IAAT,CAAc,OAAhC;AACA,QAAA,KAAI,CAAC,QAAL,GAAgB,EAAhB;;AACA,aAAI,IAAI,CAAR,IAAa,GAAG,CAAC,IAAJ,CAAS,IAAT,CAAc,OAA3B,EAAoC;AAClC,UAAA,KAAI,CAAC,QAAL,CAAc,IAAd,CAAmB,GAAG,CAAC,IAAJ,CAAS,IAAT,CAAc,OAAd,CAAsB,CAAtB,CAAnB;AACF;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ,EAAkB,KAAI,CAAC,QAAvB;AACA,QAAA,KAAI,CAAC,SAAL,GAAiB,IAAjB;AACD,OAdD,EAcG,KAdH,CAcS,UAAC,GAAD,EAAS;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;AACD,OAhBD;AAiBD,KA1BM;AA2BP;AACA,IAAA,UA5BO,wBA4BM;AACX,WAAK,cAAL,GAAsB,IAAtB;AACA,WAAK,SAAL,GAAiB,KAAjB;AACD,KA/BM;AAgCP;AACA,IAAA,OAjCO,qBAiCG;AACR,WAAK,cAAL,GAAsB,KAAtB;AACD,KAnCM;AAoCP;AACA,IAAA,QArCO,sBAqCI;AACT,WAAK,SAAL,GAAiB,KAAjB;AACD,KAvCM;AAwCP;AACA,IAAA,UAzCO,wBAyCM;AACX,WAAK,YAAL,GAAoB,IAApB,CADW,CAEX;;AACA,WAAK,IAAI,CAAT,IAAc,KAAK,QAAnB,EAA6B;AAC3B,YAAI,CAAC,KAAK,QAAL,CAAc,CAAd,CAAL,EAAuB;AACrB,eAAK,UAAL,GAAkB,OAAlB;AACA,eAAK,SAAL,GAAiB,IAAjB;AACA,eAAK,YAAL,GAAoB,KAApB;AACA;AACF;AACF,OAVW,CAWX;;;AACA,WAAK,MAAL,CAAY,MAAZ,CAAmB,iBAAnB,EAAsC,KAAK,QAA3C,EAZW,CAaX;;AACA,WAAK,OAAL,CAAa,IAAb,CAAkB;AAChB,QAAA,IAAI,EAAE;AADU,OAAlB;AAGD;AA1DM;AArDI,CAAf","sourcesContent":["<template>\n  <div class=\"list_page\">\n    <el-tag @click=\"showTipOne()\">TipOne</el-tag>\n    <el-tag type=\"success\" @click=\"showTipTwo()\">TipTwo</el-tag>\n    <el-tag type=\"warning\" @click=\"showTipThree()\">TipThree</el-tag>\n\n    <!-- TipOne_register -->\n    <el-card class=\"box-card\" v-show=\"ifShowRegister\">\n      <template #header>\n        <div class=\"card-header\">\n          <span>Register</span>\n          <el-button class=\"button\" type=\"text\" @click=\"onClose()\">\n            close\n          </el-button>\n        </div>\n      </template>\n      <div class=\"register_box\">\n        <el-form\n          class=\"register_form\"\n          ref=\"ruleForm\"\n          :model=\"ruleForm\"\n          :rules=\"rules\"\n          label-width=\"120px\"\n        >\n          <el-form-item label=\"你的名字\" prop=\"name\">\n            <el-input\n              v-model=\"ruleForm.name\"\n              placeholder=\"请输入你的名字\"\n            ></el-input>\n          </el-form-item>\n          <el-form-item label=\"手机号\" prop=\"phone\">\n            <el-input\n              v-model=\"ruleForm.phone\"\n              placeholder=\"请输入手机号\"\n            ></el-input>\n          </el-form-item>\n          <el-form-item label=\"验证码\" prop=\"valid_code\">\n            <div class=\"valid_box\">\n              <el-input v-model=\"ruleForm.valid_code\"></el-input>\n              <valid-code></valid-code>\n            </div>\n          </el-form-item>\n          <el-form-item label=\"密码\" prop=\"password\">\n            <el-input\n              v-model=\"ruleForm.password\"\n              placeholder=\"请输入密码\"\n            ></el-input>\n          </el-form-item>\n        </el-form>\n        <el-button\n          class=\"register_btn\"\n          type=\"primary\"\n          :disabled=\"btn_disabled\"\n          plain\n          @click=\"onRegister()\"\n          >Register</el-button\n        >\n      </div>\n    </el-card>\n\n    <!-- TipTwo_img -->\n    <div v-show=\"ifShowImg\" v-for=\"(item) in img_list\" :key=\"item.id\">\n      <el-image :src=\"item.url\"></el-image>\n    </div>\n    <!-- 提示框 -->\n    <el-alert\n      v-show=\"ifShowTip\"\n      center\n      :title=\"tipContent\"\n      type=\"warning\"\n      @close=\"closeTip()\"\n    ></el-alert>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport ValidCode from \"@/components/VerificationCode\";\n// import {\n//   mapActions,\n//   mapState\n// } from 'vuex';\nexport default {\n  components: {\n    ValidCode,\n  },\n  data() {\n    return {\n      ifShowImg: false, //是否显示图片\n      img_list: [], //图片列表\n      tipContent: \"\", //提示内容\n      ifShowTip: false, //是否显示提示窗\n      btn_disabled: false, //注册按钮调接口防并发\n      codeList: [],\n      ifShowRegister: true, //是否显示注册框\n      ruleForm: {\n        //表单输入项\n        name: \"\",\n        phone: \"\",\n        valid_code: \"\",\n        password: \"\",\n      },\n      rules: {\n        name: [\n          {\n            required: true,\n            message: \"Please input your name\",\n            trigger: \"blur\",\n          },\n        ],\n        phone: [\n          {\n            required: true,\n            message: \"Please input your phone\",\n            trigger: \"blur\",\n          },\n        ],\n        valid_code: [\n          {\n            required: true,\n            message: \"Please input valid code\",\n            trigger: \"blur\",\n          },\n        ],\n        password: [\n          {\n            required: true,\n            message: \"Please input your password\",\n            trigger: \"blur\",\n          },\n        ],\n      },\n    };\n  },\n  mounted() {},\n  methods: {\n    // 打开第三项——未知页面\n    showTipThree() {\n      this.ifShowRegister = false;\n      this.ifShowImg = false\n    },\n    // 打开第二项——未知页面\n    showTipTwo() {\n      this.ifShowRegister = false;\n      axios({\n        url: \"/api/getList\",\n        headers: {\n          \"Content-Type\": \"application/json\",\n        },\n      }).then((res) => {\n        console.log(res)\n        console.log(\"xxx\",res.data.body.listImg);\n        this.img_list = []\n        for(let i in res.data.body.listImg) {\n          this.img_list.push(res.data.body.listImg[i]);\n        }\n        console.log(\"ddd\",this.img_list);\n        this.ifShowImg = true\n      }).catch((rej) => {\n        console.log(rej);\n      });\n    },\n    // 打开第一项——注册页面\n    showTipOne() {\n      this.ifShowRegister = true;\n      this.ifShowImg = false;\n    },\n    // 关闭注册页面\n    onClose() {\n      this.ifShowRegister = false;\n    },\n    // 关闭提示\n    closeTip() {\n      this.ifShowTip = false;\n    },\n    // 点击注册\n    onRegister() {\n      this.btn_disabled = true;\n      // 判断所填的值为空则提示完善\n      for (let i in this.ruleForm) {\n        if (!this.ruleForm[i]) {\n          this.tipContent = \"请完善信息\";\n          this.ifShowTip = true;\n          this.btn_disabled = false;\n          return;\n        }\n      }\n      // 存值\n      this.$store.commit(\"setRegisterInfo\", this.ruleForm);\n      // 跳转到about页面 查看注册的消息\n      this.$router.push({\n        name: \"About\",\n      });\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n.list_page {\n  .el-tag {\n    margin: 15px;\n  }\n  .card-header {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n  }\n  .register_box {\n    height: auto;\n    .register_btn {\n      width: 70%;\n    }\n    .register_form {\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n      justify-content: center;\n      // background: #ccc;\n      padding-right: 15%;\n      .valid_box {\n        // background: #ccc;\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        overflow: hidden;\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":""}]}